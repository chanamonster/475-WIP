<html>
	<head>
		<title>Housemates App</title>
    	<link rel="icon" type="image/ico" href="images/logo.ico"/>
    	<link href='http://fonts.googleapis.com/css?family=Open+Sans:300,700,400' rel='stylesheet' type='text/css'>
   		<link href='http://fonts.googleapis.com/css?family=Pacifico' rel='stylesheet' type='text/css'>
    	<link rel="stylesheet" href="css/home.css">
    	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
   		<script src="js/bootstrap.min.js"></script>
   		<script src="js/script.js"></script>
	</head>
	<body>
	    <div id="signup-box">
		<h2>Signup</h2>

		<!-- SIGNUP FORM -->
		<form id="signup-form">
			<div class="form-group">
				<label>Email</label>
				<input id="signup-email" type="text" class="form-control" name="email" placeholder="Email">
			</div>
			<div class="form-group">
				<label>Password</label>
				<input id="signup-password" type="password" class="form-control" name="password" placeholder="Password">
			</div>
			<!-- <div class="form-group">
				<label>Name</label>
				<input type="text" class="form-control" name="name">
			</div> -->

			<button id="button1" type="submit" class="btn btn-warning btn-lg">Signup</button>
		</form>

		</div>
		Already have an account? <a href="/"><input id="button2" value="Login"></a><br>

	</div>
	</body>
</html>
